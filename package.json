{
  "name": "pondjs",
  "version": "0.4.0",
  "description": "A timeseries library build on top of immutable.js",
  "options": {
    "mocha": "--require scripts/mochaboot test/*.js"
  },
  "scripts": {
    "lint": "eslint src test/tests website/modules/*.jsx",
    "test": "npm run lint && mocha $npm_package_options_mocha",
    "test-only": "mocha $npm_package_options_mocha",
    "build": "rm -rf lib/* && babel src --optional runtime --stage 0 --out-dir lib",
    "build-website": "webpack --config webpack.website.config.js",
    "deploy-website": "gh-pages-deploy",
    "start-website": "webpack-dev-server --content-base website/ --config webpack.website.config.js",
    "start-tester": "webpack-dev-server --config webpack.test.config.js --content-base ./devserver --port 9500 --colors"
  },
  "pre-commit": [
    "lint",
    "test-only",
    "build"
  ],
  "babel": {
    "optional": [
      "runtime",
      "es7.objectRestSpread"
    ]
  },
  "gh-pages-deploy": {
    "staticpath": "website",
    "prep": [
      "build-website"
    ]
  },
  "license": "BSD-3-Clause-LBNL",
  "dependencies": {
    "babel-runtime": "~5.8.3",
    "immutable": "^3.6.4",
    "immutable-devtools": "0.0.4",
    "moment": "^2.9.0",
    "underscore": "^1.8.2"
  },
  "devDependencies": {
    "babel": "^5.8.3",
    "babel-core": "^5.1.13",
    "babel-eslint": "^4.1.5",
    "babel-loader": "^5.0.0",
    "bundle-loader": "^0.5.0",
    "chai": "^3.0.0",
    "css-loader": "^0.9.0",
    "eslint": "^1.10.0",
    "eslint-config-esnet": "^0.1.0",
    "eslint-plugin-babel": "^2.1.1",
    "eslint-plugin-react": "^3.9.0",
    "file-loader": "^0.7.2",
    "gh-pages-deploy": "^0.3.0",
    "json-loader": "^0.5.1",
    "jsx-loader": "^0.13.x",
    "mocha": "^2.2.5",
    "mocha-loader": "^0.7.1",
    "pre-commit": "^1.1.2",
    "raw-loader": "^0.5.1",
    "react": "^0.13.x",
    "react-markdown": "^1.0.3",
    "react-markdown-el": "^1.0.2",
    "react-router": "^0.13.x",
    "react-timeseries-charts": "^0.4.0",
    "ringjs": "0.0.1",
    "style-loader": "^0.8.0",
    "url-loader": "^0.5.5",
    "webpack": "^1.7.3",
    "webpack-dev-server": "^1.6.5"
  },
  "keywords": [
    "timeseries"
  ]
}
